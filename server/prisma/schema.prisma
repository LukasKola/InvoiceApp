// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema


generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}


model KlaraInvoices {
  id Int @default(autoincrement()) @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  payDate DateTime
  variabileSymbol Int
  paymentType String
  customer String
  service String
  totalPrice String
  qrCode String
}

model SonaInvoices {
  id Int @default(autoincrement()) @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  payDate DateTime
  variabileSymbol Int
  paymentType String
  customer String
  service String
  totalPrice String
  qrCode String
}

